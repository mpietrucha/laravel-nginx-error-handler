{
    "name": "mpietrucha/laravel-nginx-error-handler",
    "description": "Error handler for pre-rendering error pages for laravel",
    "type": "liblary",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mpietrucha\\Nginx\\Error\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Micha≈Ç Pietrucha",
            "email": "michal.pietrucha@off-site.pl"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "mpietrucha/support": "^1.0",
        "mpietrucha/composer": "^1.0",
        "symfony/http-foundation": "^6.2",
        "mpietrucha/minify": "^1.0",
        "nesbot/carbon": "^2.66"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mpietrucha\\Nginx\\Error\\NginxErrorServiceProvider"
            ]
        }
    }
}
